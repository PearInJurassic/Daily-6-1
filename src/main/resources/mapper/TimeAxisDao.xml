<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.massizhi.daily.dao.TimeAxisDao">
    <select id="getTimeAxisTypeById" resultType="String">
        SELECT time_axis_type
        FROM tb_time_axis
        WHERE
        time_axis_id=#{timeAxisId}
    </select>
</mapper>