<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.massizhi.daily.dao.AreaDao">
    <select id="getAreaNameById" resultType="String">
        SELECT area_name
        FROM tb_area
        WHERE
        area_id=#{areaId}
    </select>
</mapper>