;AACA,aAAA,IAAA,KAAA,IAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA,CAAA,KAAA,CAAA;AACA,SARA,CASA;;AACA,OAfA,EAgBA,KAhBA,CAgBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAlBA;AAmBA,KA5BA;AA6BA,IAAA,iBA7BA,+BA6BA;AACA,WAAA,MAAA;AACA;AA/BA,GAhBA;AAiDA,EAAA,OAjDA,qBAiDA;AACA,SAAA,IAAA;AACA,GAnDA;AAoDA,EAAA,OApDA,qBAoDA;AAAA;;AACA,IAAA,GAAA,CAAA,GAAA,CAAA,YAAA,EAAA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA;AACA,aAAA,QAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,MAAA;;AACA;;AACA;AACA;AANA,OADA,CASA;;AACA,KAVA;AAWA,IAAA,GAAA,CAAA,GAAA,CAAA,cAAA,EAAA,UAAA,IAAA,EAAA;AACA,MAAA,MAAA,CAAA,OAAA,GAAA,EAAA;;AACA,WAAA,IAAA,KAAA,IAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA,CAAA;AACA;;AACA,MAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,KANA;AAOA,GAvEA;AAwEA,EAAA,KAAA,EAAA;AACA,gCAAA,gCAAA,GAAA,EAAA;AAAA;AACA,WAAA,WAAA,GAAA,GAAA;AACA;AAHA;AAxEA,CAAA","sourcesContent":["<template>\r\n    <div class=\"ContentAll\">\r\n        <div class=\"ContentCenter\" v-loading=\"loading\">\r\n            <component :is=\"PostAll\"\r\n                       :key=\"index\"\r\n                       :itemInfo=\"postNum[index]\"\r\n                       :likeInfo =\"likeList[index]\"\r\n                       @deleteSuccess=\"reloadAfterDelete\"\r\n                       v-for=\"(item,index) in postNum\">\r\n            </component>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n  import PostAll from \"@/pages/PostPage/components/Post/PostAll\";\r\n  import Bus from \"@/JS/bus.js\";\r\n\r\n  export default {\r\n    inject: ['reload'],\r\n    name: \"ContentPostMain\",\r\n    data() {\r\n      return {\r\n        windowWidth:document.documentElement.clientWidth,\r\n        PostAll: \"PostAll\",\r\n        postNum: [],\r\n        img:\"\",\r\n        likeList:[],\r\n        loading:true\r\n      }\r\n    },\r\n    components: {\r\n      PostAll,\r\n    },\r\n    methods: {\r\n      /**\r\n       * @description 初始化渲染帖子列表。\r\n       */\r\n      init() {\r\n        // let url=location.search;//获取url\r\n        // let userID = url.split('=')[1];\r\n        // sessionStorage.setItem('ID',userID);\r\n        let userID = sessionStorage.getItem(\"ID\");\r\n        this.axios.get(`${this.GLOBAL.apiUrl}/listpost`,{\r\n          params:{\r\n            userId:userID,\r\n          }\r\n        })\r\n          .then((response) => {\r\n            console.log(response)\r\n            this.loading=false\r\n            this.likeList = [...response.data.likeList];\r\n            // console.log(this.likeList)\r\n            let postData = response.data.postList;\r\n            for(let index in postData) {\r\n              this.postNum.push(postData[index])\r\n            }\r\n            // console.log(this.postNum)\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n      },\r\n      reloadAfterDelete() {\r\n        this.reload();\r\n      }\r\n    },\r\n    created() {\r\n        this.init();\r\n    },\r\n    mounted() {\r\n      Bus.$on(\"finishEdit\", (flag) => {\r\n        switch (flag) {\r\n            case 'finish':\r\n              console.log(\"finish\")\r\n              this.reload();\r\n              break;\r\n            default:\r\n              break;\r\n        }\r\n        // console.log('success')\r\n      });\r\n      Bus.$on(\"finishSearch\",(list) =>{\r\n        this.postNum=[];\r\n        for(let index in list) {\r\n          this.postNum.push(list[index])\r\n        }\r\n        this.loading=false;\r\n      })\r\n    },\r\n    watch:{\r\n      '$store.state.screenWidth':function(val){ //监听屏幕宽度变化\r\n        this.windowWidth=val;\r\n      }\r\n    },\r\n  }\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n    @outlineColor: #989899;\r\n    .ContentAll {\r\n        display: flex;\r\n        max-width: 1025px;\r\n        min-width: 350px;\r\n    }\r\n\r\n    .ContentCenter {\r\n        max-width: 1025px;\r\n        min-width: 350px;\r\n\r\n        padding-top: 30px;\r\n    }\r\n</style>"],"sourceRoot":"src/pages/PostPage/components/Content"}]}